<h1>Create a New Bulletin Post</h1>

<%= form_for(@bulletin_post) do |f| %>
   <div class="field">
      <%= f.label :title %>
      <%= f.text_field :title %>
   </div>
   
   <div class="field">
      <%= f.label :body %>
      <%= f.text_field :body %>
   </div>
   
   <div class="actions">
      <%= f.submit %>
   </div>
<% end %>
  
  